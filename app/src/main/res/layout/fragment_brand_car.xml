    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:context="com.bwf.tuanche.ui.carselect.fragment.BrandCarFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/sl_brandcar_select">
        <LinearLayout
            android:id="@+id/ll_brandcar_select"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </ScrollView>
    
</LinearLayout>
